{
    "version": 1,
    "disable_existing_loggers": false,
    "formatters": {
        "simple": {
            "format": "%(levelname)s:%(name)s:%(message)s\n"
            },
        "time": {
            "format": "%(asctime)s : %(levelname)s : %(name)s : %(message)s\n"
        }
    },

    "handlers": {
        "console": {
            "class": "logging.StreamHandler",
            "level": "DEBUG",
            "formatter": "simple",
            "stream": "ext://sys.stdout"
        }


    },

    "root": {
        "level": "DEBUG",
        "handlers": ["console" ]
    }
}